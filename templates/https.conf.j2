ssl on;
ssl_certificate {{ host_ssl_certificate }};
ssl_certificate_key {{ host_ssl_cerfificate_key }};

ssl_session_timeout 5m;

ssl_protocols {{ host_ssl_protocols }};
ssl_ciphers '{{ host_ssl_ciphers }}';

ssl_prefer_server_ciphers on;

{% if host_ssl_dhparam %}
ssl_dhparam {{ host_ssl_dhparam }};
{% endif %}
